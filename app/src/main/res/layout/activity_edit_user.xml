<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.EditUserActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/activity_EditUser_toolbar"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteY="4dp" />

    <TextView
        android:id="@+id/activity_EditUser_RSR"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="RSR"
        android:textColor="#000000"
        android:textSize="40dp"
        app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_RSR_full"
        app:layout_constraintHorizontal_bias="0.831"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_toolbar" />

    <TextView
        android:id="@+id/activity_EditUser_RSR_full"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="96dp"
        android:text="Realtime seat\nreservation"
        android:textColor="#000000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_toolbar" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/activity_EditUser_constraintLayout1"
        android:layout_width="415dp"
        android:layout_height="343dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_RSR_full">

        <TextView
            android:id="@+id/activity_EditUser_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="Name"
            app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_email"
            app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_constraintLayout2"
            app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toTopOf="@+id/activity_EditUser_constraintLayout1" />

        <TextView
            android:id="@+id/activity_EditUser_email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="e-mail"
            app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_phone"
            app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_constraintLayout2"
            app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_name" />

        <TextView
            android:id="@+id/activity_EditUser_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="Phone"
            app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_password"
            app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_constraintLayout2"
            app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_email" />

        <TextView
            android:id="@+id/activity_EditUser_password"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="password"
            android:textColor="@color/colorPrimary"
            app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_confirm"
            app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_constraintLayout2"
            app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_phone" />

        <TextView
            android:id="@+id/activity_EditUser_confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:text="confirm"
            android:textColor="@color/colorPrimary"
            app:layout_constraintBottom_toBottomOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintEnd_toStartOf="@+id/activity_EditUser_constraintLayout2"
            app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_password" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/activity_EditUser_constraintLayout2"
            android:layout_width="247dp"
            android:layout_height="331dp"
            app:layout_constraintBottom_toBottomOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout1"
            app:layout_constraintTop_toTopOf="@+id/activity_EditUser_constraintLayout1">

            <View
                android:id="@+id/activity_EditUser_divider1"
                android:layout_width="245dp"
                android:layout_height="1dp"
                android:background="#00000000"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider2"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toTopOf="@+id/activity_EditUser_constraintLayout2" />

            <View
                android:id="@+id/activity_EditUser_divider2"
                android:layout_width="245dp"
                android:layout_height="1dp"
                android:background="#00000000"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider3"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider1" />

            <View
                android:id="@+id/activity_EditUser_divider3"
                android:layout_width="245dp"
                android:layout_height="1dp"
                android:background="#00000000"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider4"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider2" />

            <View
                android:id="@+id/activity_EditUser_divider4"
                android:layout_width="245dp"
                android:layout_height="1dp"
                android:background="#00000000"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider5"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider3" />

            <View
                android:id="@+id/activity_EditUser_divider5"
                android:layout_width="245dp"
                android:layout_height="1dp"
                android:background="#00000000"
                app:layout_constraintBottom_toBottomOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2" />

            <EditText
                android:id="@+id/activity_EditUser_editName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Minki Kim"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider1"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toTopOf="@+id/activity_EditUser_constraintLayout2" />

            <EditText
                android:id="@+id/activity_EditUser_editEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:text="rlaalsrl7777@naver.com"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider2"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider1" />

            <EditText
                android:id="@+id/activity_EditUser_editPhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:ems="10"
                android:inputType="phone"
                android:text="010-1234-5678"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider3"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider2" />

            <EditText
                android:id="@+id/activity_EditUser_editPassword"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:ems="10"
                android:inputType="numberPassword"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider4"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider3" />

            <EditText
                android:id="@+id/activity_EditUser_editconfirm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="16dp"
                android:layout_marginBottom="16dp"
                android:ems="10"
                android:inputType="numberPassword"
                app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_divider5"
                app:layout_constraintEnd_toEndOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintStart_toStartOf="@+id/activity_EditUser_constraintLayout2"
                app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_divider4" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <Button
        android:id="@+id/activity_EditUser_save_change_button"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:background="@drawable/primary_rounded_botton"
        android:text="Save Changes"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/activity_EditUser_logout_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_constraintLayout1" />

    <Button
        android:id="@+id/activity_EditUser_logout_button"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:background="@drawable/accent_rounded_botton"
        android:text="Log Out"
        android:textColor="#FFFFFF"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_EditUser_save_change_button" />
</androidx.constraintlayout.widget.ConstraintLayout>